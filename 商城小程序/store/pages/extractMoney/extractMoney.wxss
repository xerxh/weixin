/* pages/extractMoney/extractMoney.wxss */
.margin-top{
  margin-top: 200rpx;
}
.topTitle{
  font-size:34rpx;
  font-family:PingFang-SC-Heavy;
  font-weight:800;
  color:rgba(0,0,0,1);
}
.flex{
  display: flex;
}
.column{
  flex-direction: column;
  padding-left: 50rpx;
  justify-content: space-between;
  border-top: 1px solid rgba(221,221,221,1);
  border-bottom: 1px solid rgba(221,221,221,1);
}
.between{
  justify-content: space-between;
  border-top: 1px solid gray;
  padding: 40rpx 50rpx;
}
.shareMoneyTitle{
  font-size:24rpx;
  margin-top: 60rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(102,102,102,1);
}
.moneyNumber{
  font-size:90rpx;
  font-family:PingFang-SC-Heavy;
  font-weight:800;
  color:rgba(0,0,0,1);
  margin-top: 50rpx;
  margin-bottom: 70rpx; 
  /* text-align: left; */
  height:100rpx;
}
.btn{
  position: fixed;
  bottom:  150rpx;
  /* width: 700rpx; */
  left: 4%;
  width:690rpx;
  height:90rpx;
  background:rgba(255,213,98,1);
  border-radius:6px;
  font-size:34rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(0,0,0,1);
}

.btnBox {
  width: 95%;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
  border-bottom: 1px solid rgba(221,221,221,1);
}
.btnNameBox{
  display: flex;
  align-items: center;
}
.btnName {
  margin-left: 18rpx;
  font-size:30rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(0,0,0,1);
}
.money{
  margin-left: 50rpx;
}
.btnBox:last-child{
  border: 0;
}
.btn_arrow{
  width:14rpx;
  height:26rpx;
}
.btnImg{
  width: 37rpx;
  height: 36rpx;
}