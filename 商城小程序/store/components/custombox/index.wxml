<!--components/custombox/index.wxml-->
<view>
  <slot></slot>
  <view class='customBox'>
  <block wx:for="{{list}}" wx:key="key" >
    <view class='childBox' bindtap='itemBtn' data-title='{{item.name}}' data-id='{{item.categoryId}}'>
      <image src='{{item.picurl}}' class='my-image-class'></image>
      <text class='my-text-class'>{{item.name}}</text>
    </view>
  </block>
  </view>
</view>
