/* components/navigator-table/index.wxss */
.bottomTabContaner{
  width: 700rpx;
  background: #CCCCCC;
  position: fixed;
  bottom: 0;
  display: flex;
  font-size: 13px;
  padding: 10rpx 30rpx 10rpx 30rpx;
  justify-content: space-between;
}
.bottomTabItem  image{
  width: 60rpx;
  height: 60rpx;
}
.btn{
  background-color: #CCCCCC;
  border: 0px;
  height: 100rpx;
  margin: 0;
  margin-top: 3rpx;
  padding: 0;
  /* display: flex; */
  overflow: visible;
  font-size: 13px;
  /* flex-direction: column;
  align-items: center; */
  line-height: 10rpx;
}
.default{
  position: relative;
  top: 20rpx;
}
.btn:after {
  border: 0px;
}
.active{
  color: orange;
}
